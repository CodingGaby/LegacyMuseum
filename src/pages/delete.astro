import {turso} from "src/turso";
import PurchaseOrderID from "./purchaseOrders/[purchaseOrderID].astro";

const purchaseOrderID = Astro.url.searchParams.get("purchaseOrderID");

await turso.execute( ` UPDATE PurchaseOrders SET statusID = 6 WHERE PurchaseOrderID = ${PurchaseOrderID} `);

return Astro.redirect('/purchaseOrders')
